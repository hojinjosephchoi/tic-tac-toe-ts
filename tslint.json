{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": {
      "options": [
        "never-prefix"
      ]
    },
    "max-classes-per-file": {
      "options": [
        10
      ]
    },
    "member-access": false,
    "no-empty-interface": false,
    "prefer-for-of": false,
    "array-type": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
